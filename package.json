{
  "name": "test",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "deploy": "npm install && npm run build && npm run start:prod"
  },
  "dependencies": {
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/mongoose": "^9.2.0",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/typeorm": "^9.0.0",
    "app-root-path": "^3.0.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "debug": "^4.3.4",
    "dotenv": "^16.0.1",
    "dynamoose": "^2.8.5",
    "express-jwt": "^7.7.5",
    "fs": "^0.0.1-security",
    "hashmap": "^2.4.0",
    "helmet": "^5.1.0",
    "mariadb": "^3.0.0",
    "moment-timezone": "^0.5.34",
    "mongoose": "^6.4.4",
    "morgan": "^1.10.0",
    "multer-s3": "^3.0.1",
    "mysql": "^2.18.1",
    "mysql2": "^2.3.3",
    "nest-morgan": "^2.0.0",
    "nest-winston": "^1.7.0",
    "nestjs-dynamoose": "^0.3.4",
    "nestjs-form-data": "^1.6.4",
    "node-uuid": "^1.4.8",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "typeorm": "^0.3.7",
    "urllib": "^3.0.4",
    "uuidv4": "^6.2.13",
    "winston": "^3.8.1",
    "winston-daily-rotate-file": "^4.7.1"
